@model IEnumerable<WorldCup.Common.Entities.Parameter>

@{
    ViewBag.Title = "Display parameters";
}
<div class="page-header">
    <h1>Application parameters<br />
    <small>Change site settings</small>
    </h1>
</div>

@using (Html.BeginForm("Parameters", "AdminGeneral", FormMethod.Post, new { role = "form", @class = "form-horizontal" }))
{
    <div class="row">
        @Html.EditorForModel()
    </div>

    <button type="submit" class="btn btn-primary">Save</button>
}